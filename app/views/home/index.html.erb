<body>

  <div>
    <h1>Rails Weather</h1>
  <div>
  
  <div>
    <form class="fetchWeather">
      <input type="text" placeholder="Search by city" name="search">
      <link id="submit "><input type="submit" value="Submit"></link>
    </form>
  </div>
  <div>
    <div class="humidity">
      <h2>Humidity</h2>
    </div> 
  
    <div class="temperature">
      <h2>Temperature</h2>
    </div> 
  
    <div class="pressure">
      <h2>Pressure</h2>
    </div>
  </div>
  <%= javascript_include_tag "application" %>
</body>


